import java.io.*;
import java.util.* ;

import java.util.ArrayList;

public class Solution 
{
	public static ArrayList<ArrayList<Long>> printPascal(int n) 
	{
		ArrayList<ArrayList<Long>> ans = new ArrayList<>();
        for (int row = 1; row <= n; row++) 
        {
            ans.add(rowgeneration(row));
        }
        return ans;
	}

	public static ArrayList<Long> rowgeneration(int row) 
    {
        long ans = 1;
        ArrayList<Long> ansr = new ArrayList<>();
        ansr.add(1L); 
        for (int col = 1; col < row; col++) 
        {
            ans = ans * (row - col);
            ans = ans / col;
            ansr.add((ans));
        }
        return ansr;
    }
}
