
import java.util.* ;
import java.io.*; 
import java.util.ArrayList;
import java.util.Collections;

public class Solution 
{
	public static ArrayList<Integer> nextPermutation(ArrayList<Integer> a) 
	{
		// Write your code here.
        int n = a.size();
        int flag=0;
        int i ;
        for( i = n-2;i>=0;i--)
        {
            if(a.get(i)<a.get(i+1))
            {
                flag=1;
                break;
            }
        }
        if(flag==0)
        {
            reverse(a,0,n-1);
        }
        else

        {
            for(int j=n-1;j>i;j--)
            {
                if(a.get(j)>a.get(i))
                {
                    int temp = a.get(j);
                    a.set(j,a.get(i));
                    a.set(i,temp);
                    break;
                }
            }
            reverse(a,i+1,n-1);
        }
    
		return a;
    }
    public static void reverse (ArrayList<Integer> a , int low, int high )
    {
        while(low<high)
        {
            int temp =a.get(low);
            a.set(low,a.get(high));
            a.set(high,temp);
            low++;
            high--;
        }
    }
}
